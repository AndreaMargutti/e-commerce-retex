{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean-project": "rm -rf .nuxt dist node_modules",
    "storybook": "storybook dev -p 6006 --no-open",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"{components,pages,plugins,middleware,layouts,composables,assets}/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@kgierke/nuxt-basic-auth": "1.7.0",
    "@nuxt/eslint": "1.4.0",
    "@nuxt/fonts": "^0.11.1",
    "@nuxt/icon": "^1.11.0",
    "@nuxt/image": "1.10.0",
    "@nuxtjs/i18n": "9.4.0",
    "@tailwindcss/vite": "^4.1.7",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-vue": "^8.6.0",
    "eslint": "^9.27.0",
    "eslint-plugin-vue": "^10.1.0",
    "lint-staged": "^16.0.0",
    "msw": "^2.7.4",
    "msw-storybook-addon": "^2.0.4",
    "nuxt": "^3.16.1",
    "tailwindcss": "^4.1.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@nuxtjs/storybook": "8.3.3",
    "@storybook-vue/nuxt": "8.3.3",
    "@storybook/addon-a11y": "8.3.3",
    "@storybook/addon-essentials": "8.3.3",
    "@storybook/addon-interactions": "8.3.3",
    "@storybook/addon-links": "8.3.3",
    "@storybook/blocks": "8.3.3",
    "@storybook/vue3": "8.3.3",
    "@tailwindcss/postcss": "^4.0.16",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "prettier": "3.5.3",
    "sass": "^1.86.0",
    "storybook": "8.3.3",
    "typescript": "^5.8.3"
  },
  "patchedDependencies": {
    "@storybook-vue/nuxt@8.3.3": "patches/@storybook-vue%2Fnuxt@8.3.3.patch"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*": [
      "bun run lint",
      "bun run lint:fix"
    ]
  }
}
